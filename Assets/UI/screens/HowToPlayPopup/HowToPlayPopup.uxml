<engine:UXML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:engine="UnityEngine.UIElements" xmlns:editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <Style src="project://database/Assets/UI/screens/HowToPlayPopup/HowToPlayPopup.uss?fileID=7433441132597879392&amp;guid=4d914b6e8261a4dda966f8c137df4cdb&amp;type=3#HowToPlayPopup" />
    <Popup name="HtpPopup" style="visibility: visible; display: flex;">
        <engine:VisualElement name="HtpPopupInner" class="htp-popup" style="flex-grow: 0;">
